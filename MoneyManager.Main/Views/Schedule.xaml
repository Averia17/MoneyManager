<UserControl x:Class="MoneyManager.Main.Views.Schedule"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:calendar="clr-namespace:WPF.EventCalendar;assembly=WPF.EventCalendar"
             x:Name="ScheduleView"
             
             xmlns:local="clr-namespace:MoneyManager.Main.Views"
             mc:Ignorable="d" 
             
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="#e2eefb">
        <Grid>
            <calendar:CalendarMonth x:Name="Calendar" Events="{Binding Events}" />

        </Grid>
    </Grid>
</UserControl>
